<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".BookAppointmentActivity">


  <TextView
      android:id="@+id/textView4"
      android:layout_width="86dp"
      android:layout_height="19dp"
      android:text="SelectTime"
      android:textColor="@color/YellowLight"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.836"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/editTextLTBPinCode"
      app:layout_constraintVertical_bias="0.121" />

  <Button
      android:id="@+id/buttonLTBBook"
      android:layout_width="281dp"
      android:layout_height="45dp"
      android:layout_marginBottom="84dp"
      android:background="@drawable/btn_bg"
      android:text="Book Appointment"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/editTextLTBPinCode"
      app:layout_constraintVertical_bias="1.0" />

  <Button
      android:id="@+id/buttonCartBack"
      android:layout_width="115dp"
      android:layout_height="46dp"
      android:layout_marginBottom="16dp"
      android:background="@drawable/btn_bg"
      android:text="BACK"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"

      app:layout_constraintTop_toBottomOf="@+id/buttonLTBBook"
      app:layout_constraintVertical_bias="0.454" />

  <EditText
      android:id="@+id/editTextLTBPinCode"
      android:layout_width="305dp"
      android:layout_height="50dp"
      android:layout_marginTop="28dp"
      android:layout_marginBottom="184dp"
      android:background="@drawable/input_bg"
      android:backgroundTint="#12FFFFFF"
      android:drawableLeft="@drawable/ic_baseline_person_24"
      android:drawablePadding="10dp"
      android:ems="10"
      android:hint="Fees"
      android:inputType="textPersonName"
      android:paddingLeft="17dp"
      android:paddingTop="12dp"
      android:paddingRight="10dp"
      android:paddingBottom="10dp"
      android:textColor="#FFFFFF"
      android:textColorHint="#DCD8D4D4"
      android:textSize="20sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/editTextLTBContactNumber"
      app:layout_constraintVertical_bias="0.0" />

  <EditText
      android:id="@+id/editTextLTBAddress"
      android:layout_width="305dp"
      android:layout_height="50dp"
      android:layout_marginTop="28dp"
      android:layout_marginBottom="324dp"
      android:background="@drawable/input_bg"
      android:backgroundTint="#0DFFFFFF"
      android:drawableLeft="@drawable/ic_baseline_security_24"
      android:drawablePadding="10dp"
      android:ems="10"
      android:hint="Address"
      android:inputType="textEmailAddress"
      android:paddingLeft="17dp"
      android:paddingTop="12dp"
      android:paddingRight="10dp"
      android:paddingBottom="10dp"
      android:textColor="#FFFFFF"
      android:textColorHint="#BFEFE9E9"
      android:textSize="20sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/editTextLTBFullName"
      app:layout_constraintVertical_bias="0.0" />

  <EditText
      android:id="@+id/editTextLTBContactNumber"
      android:layout_width="305dp"
      android:layout_height="50dp"
      android:layout_marginTop="28dp"
      android:layout_marginBottom="360dp"
      android:background="@drawable/input_bg"
      android:backgroundTint="#12FFFFFF"
      android:drawableLeft="@drawable/ic_baseline_email_24"
      android:drawablePadding="10dp"
      android:ems="10"
      android:hint="Contact Number"
      android:inputType="textPersonName"
      android:paddingLeft="17dp"
      android:paddingTop="12dp"
      android:paddingRight="10dp"
      android:paddingBottom="10dp"
      android:textColor="#FFFFFF"
      android:textColorHint="#DCD8D4D4"
      android:textSize="20sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/editTextLTBAddress"
      app:layout_constraintVertical_bias="0.0" />

  <EditText
      android:id="@+id/editTextLTBFullName"
      android:layout_width="305dp"
      android:layout_height="50dp"
      android:background="@drawable/input_bg"
      android:backgroundTint="#0DFFFFFF"
      android:drawableLeft="@drawable/ic_baseline_person_24"
      android:drawablePadding="10dp"
      android:ems="10"
      android:hint="Full Name"
      android:inputType="textPersonName"
      android:paddingLeft="17dp"
      android:paddingTop="12dp"
      android:paddingRight="10dp"
      android:paddingBottom="10dp"
      android:textColor="#FFFFFF"
      android:textColorHint="#BFEFE9E9"
      android:textSize="20sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.252" />

  <TextView
      android:id="@+id/textView"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="24/7 Health Care"
      android:textColor="#AAB2EA"
      android:textSize="34sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.043" />

  <TextView
      android:id="@+id/textViewAppTitle"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Book Appointment"
      android:textColor="#D2FFFFFF"
      android:textSize="30sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.496"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.128" />

  <TextView
      android:id="@+id/textView3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Select Date"
      android:textColor="@color/YellowLight"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.155"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/editTextLTBPinCode"
      app:layout_constraintVertical_bias="0.121" />

  <Button
      android:id="@+id/buttonCartDatePicker"
      style="?android:spinnerStyle"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="27/10/2023"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.184"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView3"
      app:layout_constraintVertical_bias="0.079" />

  <Button
      android:id="@+id/buttonAppTime"
      style="?android:spinnerStyle"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="07:26"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.835"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView4"
      app:layout_constraintVertical_bias="0.079" />

</androidx.constraintlayout.widget.ConstraintLayout>